{
  "Group": "NightRaid",
  "Name": "NCRPG",
  "Version": "${version}",
  "Description": "${description}",
  "Authors": [
    {
      "Name": "Kielian (NadoHimself)",
      "Email": "contact@nightraid.net",
      "Url": "https://nightraid.net"
    }
  ],
  "Website": "https://github.com/NadoHimself/NCRPG",
  "Main": "net.nightraid.ncrpg.NCRPG",
  "ServerVersion": "*",
  "Dependencies": {},
  "OptionalDependencies": {},
  "DisabledByDefault": false,
  "Permissions": {
    "ncrpg.skills": {
      "description": "View skill levels",
      "default": true
    },
    "ncrpg.stats": {
      "description": "View detailed skill stats",
      "default": true
    },
    "ncrpg.mcrank": {
      "description": "View leaderboards",
      "default": true
    },
    "ncrpg.party": {
      "description": "Use party system",
      "default": true
    },
    "ncrpg.admin": {
      "description": "Admin commands",
      "default": false
    }
  },
  "Commands": {
    "skills": {
      "description": "View all your skill levels",
      "permission": "ncrpg.skills",
      "usage": "/skills"
    },
    "stats": {
      "description": "View detailed stats for a specific skill",
      "permission": "ncrpg.stats",
      "usage": "/stats <skill>"
    },
    "mcrank": {
      "description": "View skill leaderboards",
      "permission": "ncrpg.mcrank",
      "usage": "/mcrank [skill]"
    },
    "party": {
      "description": "Party management commands",
      "permission": "ncrpg.party",
      "usage": "/party <create|invite|accept|leave|list>"
    }
  }
}
